  <body class="text-center">
    
<main class="form-signin shadow-lg p-3">
  <form id="formLogin" action="/login" method="POST">
    <img class="mb-4" src="img/logo.png" alt="" width="122" height="107">
    <h1 class="h3 mb-3 fw-normal">Ventas</h1>

    <div class="form-floating">
      <input type="email" class="form-control" id="user" name="user" placeholder="name@example.com">
      <label for="user">Email address</label>
    </div>


    <div class="form-floating">
      <input type="password" class="form-control" id="pass" name="pass" placeholder="Password">
      <label for="pass">Contrase√±a</label>
    </div>

    <button class="w-100 btn btn-lg btn-primary" type="submit">Iniciar Secion</button>
  </form>
</main>
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
  <% if(alert) { %>
    <script>
      Swal.fire({
        title: '<%= alertTitle %>',
        text:'<%= alertMessage %>',
        icon:'<%= alertIcon %>',
        showConfirmButton: <%= showConfirmButton %>,
        timer: <%= timer %>
      }).then(()=>{
        window.location = '/<%= ruta %>'
      })
    </script>
  <% } %>
    
  </body>
